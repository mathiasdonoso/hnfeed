extends layout.pug

block content
  h1 HN Feed
  h4 We <3 hacker news!
  table.table
    tbody
      each news in newsList
        tr
          th #{news.title} - #{news.author} -
          th #{news.createdAt}
          th
            form(method='POST', action=`/${news._id}`)
              button.btn-default(type='submit')  
                i.fas.fa-trash-alt
